{
  "name": "vite-pug-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite dev",
    "bootstrap": "npx pnpm install",
    "build": "vite build --emptyOutDir",
    "build:prod": "vite build --emptyOutDir --mode production",
    "build:staging": "vite build --emptyOutDir --staging",
    "preview": "vite preview",
    "eslint": "eslint --ext .js,.ts src",
    "eslint:fix": "eslint --ext .js,.ts src --fix",
    "prepare": "husky install",
    "commit": "git add . && git-cz"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "autoprefixer": "^10.0.2",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-pug": "^1.2.4",
    "eslint-plugin-standard": "^5.0.0",
    "glob": "^8.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "picocolors": "~1",
    "postcss": "^8.4.12",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.5.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.6.2",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.0",
    "vite-plugin-pug": "^0.3.1",
    "vite-plugin-windicss": "^1.8.4",
    "windicss": "^3.5.3"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "tailwindcss": "^3.0.23",
    "vue": "^3.2.31"
  },
  "lint-staged": {
    "*.{vue,js,ts,pug}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
